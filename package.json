{
  "name": "edge-stacks",
  "version": "1.0.1",
  "description": "Stacks implementation for Edge",
  "main": "./build/index.js",
  "files": [
    "build/src",
    "build/index.js",
    "build/index.d.ts"
  ],
  "type": "module",
  "exports": {
    ".": "./build/index.js"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "clean": "del-cli build",
    "typecheck": "tsc --noEmit",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "release": "np",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels .github/labels.json edge-js/stacks",
    "quick:test": "node --enable-source-maps --loader=ts-node/esm bin/test.ts"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "keywords": [],
  "author": "virk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/eslint-config": "^1.1.8",
    "@adonisjs/prettier-config": "^1.1.8",
    "@adonisjs/tsconfig": "^1.1.8",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@japa/assert": "^2.0.0-1",
    "@japa/runner": "^3.0.0-6",
    "@swc/core": "^1.3.78",
    "@types/node": "^20.5.1",
    "c8": "^8.0.1",
    "del-cli": "5.0.0",
    "edge.js": "^6.0.0-8",
    "eslint": "^8.36.0",
    "github-label-sync": "^2.3.1",
    "husky": "^8.0.3",
    "np": "^8.0.4",
    "openapi-types": "^12.1.3",
    "prettier": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "edge.js": "^6.0.0-7"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false
  },
  "types": "./build/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "edge-error": "^3.0.0",
    "edge-parser": "^8.2.2",
    "nanoid": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edge-js/stacks.git"
  },
  "bugs": {
    "url": "https://github.com/edge-js/stacks/issues"
  },
  "homepage": "https://github.com/edge-js/stacks#readme",
  "prettier": "@adonisjs/prettier-config",
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  }
}
